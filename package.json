{
  "name": "vexa",
  "displayName": "Vexa",
  "version": "8.02",
  "description": "Vexa - AI video call recording and summary",
  "author": "Vexa",
  "scripts": {
    "dev": "plasmo dev",
    "dev:verbose": "plasmo dev --verbose",
    "build": "plasmo build",
    "build:zip": "npm run build -- --zip",
    "package": "plasmo package"
  },
  "dependencies": {
    "@mdxeditor/editor": "^3.6.1",
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.10.0",
    "@types/audioworklet": "^0.0.55",
    "markdown-to-jsx": "^7.4.7",
    "plasmo": "0.86.1",
    "react": "18.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "18.2.0",
    "react-draggable": "^4.4.6",
    "react-dropdown-select": "^4.11.2",
    "react-loader-spinner": "^6.1.6",
    "react-notifications": "^1.7.4",
    "react-tabs": "^6.0.2",
    "react-timer-hook": "^3.0.7",
    "react-usestateref": "^1.0.8",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@parcel/packager-ts": "2.9.3",
    "@storybook/react": "^8.0.9",
    "@tailwindcss/typography": "^0.5.13",
    "@types/chrome": "0.0.267",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "postcss": "8.4.33",
    "postcss-import": "^16.1.0",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "pnpm": {
    "overrides": {
      "@parcel/runtime-js": "2.8.3"
    }
  },
  "resolutions": {
    "@parcel/runtime-js": "2.8.3"
  },
  "manifest": {
    "permissions": [
      "scripting",
      "offscreen",
      "tabs",
      "tabCapture",
      "webNavigation"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "js/*.js"
        ],
        "matches": [
          "https://meet.google.com/*"
        ]
      }
    ]
  }
}
