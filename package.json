{
  "name": "vexa",
  "displayName": "Vexa",
  "version": "0.0.1",
  "description": "Vexa - AI video call recording and summaary",
  "author": "Vexa",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.10.0",
    "@types/audioworklet": "^0.0.55",
    "plasmo": "0.86.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropdown-select": "^4.11.2",
    "react-tabs": "^6.0.2",
    "react-usestateref": "^1.0.8",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@parcel/packager-ts": "2.9.3",
    "@storybook/react": "^8.0.9",
    "@types/chrome": "0.0.267",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "pnpm": {
    "overrides": {
      "@parcel/runtime-js": "2.8.3"
    }
  },
  "resolutions": {
    "@parcel/runtime-js": "2.8.3"
  },
  "manifest": {
    "permissions": [
      "offscreen",
      "tabCapture",
      "activeTab"
    ],
    "host_permissions": [
      "https://*/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "~lib/services/volume-processor.js"
        ],
        "matches": ["<all_urls>"]
      }
    ]
  }
